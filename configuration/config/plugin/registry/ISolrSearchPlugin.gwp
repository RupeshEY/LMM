<?xml version="1.0"?>
<plugin
  interface="ISolrSearchPlugin"
  name="ISolrSearchPlugin">
  <plugin-gosu
    gosuclass="gw.solr.CCSolrSearchPlugin">
    <param
      name="debug"
      value="true"/>
    <param
      name="fetchSize"
      value="100"/>
    <param
      name="chunkSize"
      value="25"/>
    <param
      name="pageIteratorCacheTimeout"
      value="300"/>
  </plugin-gosu>
</plugin>