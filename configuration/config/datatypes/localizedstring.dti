<?xml version="1.0"?>
<DataTypeDef xmlns="http://guidewire.com/datatype" type="gw.datatype.impl.LocalizedStringDataTypeDef" valueType="java.lang.String">
  <ParameterDef name="validator" desc="The name of a ValidatorDef (fieldvalidators.xml) that should be used to validate values of this data type." required="true" type="java.lang.String"/>
  <ParameterDef name="size" desc="The maximum length allowed by this data type's constraints, and the length of the associated database column." required="true" type="java.lang.Integer"/>
  <ParameterDef name="encryption" desc="Whether values of this data type are encrypted in the database. Defaults to false." required="false" type="java.lang.Boolean"/>
  <ParameterDef name="trimwhitespace" desc="Whether to trim leading and trailing whitespace from values of this data type. Defaults to true." required="false" type="java.lang.Boolean"/>
  <ParameterDef name="countryproperty" desc="The name of a property on the owning entity, whose value contains the country with which to validate and format values." required="true" type="java.lang.String"/>
</DataTypeDef>