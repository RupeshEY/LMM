<?xml version="1.0"?>
<typelist
  xmlns="http://guidewire.com/typelists"
  desc="Standard states for a exposure, such as open or closed"
  final="true"
  name="ExposureState">
  <typecode
    code="draft"
    desc="Draft"
    name="Draft"
    priority="1"/>
  <typecode
    code="open"
    desc="Open"
    name="Open"
    priority="2"/>
  <typecode
    code="closed"
    desc="Closed"
    name="Closed"
    priority="3"/>
  <typecode
    code="exception"
    desc="None of the above"
    name="Exception"
    priority="4"/>
</typelist>