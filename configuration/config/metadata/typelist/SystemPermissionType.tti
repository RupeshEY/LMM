  <typecode
    code="retrymessage"
    desc="Permission to try to resend the failed message"
    name="Retry message"/>
  <typecode
    code="skipmessage"
    desc="Permission to skip the failed message"
    name="Skip message"/>
  <typecode
    code="resyncmessage"
    desc="Permission to resync message"
    name="Resync message"/>
