    code="scrprmview"
    desc="Permission to view the list of script parameters or details of an individual script parameter"
    name="View script parameters"/>
  <typecode
    code="scrprmmanage"
    desc="Permission to create, edit, or delete script parameters"
    name="Manage script parameters"/>
