    code="actqueuenext"
    desc="Permission to get the next activity off of a queue"
    name="Get next activity from queue"/>
  <typecode
    code="actqueuepick"
    desc="Permission to pick an activity from a queue"
    name="Pick activity from queue"/>
  <typecode
    code="actqueueassign"
    desc="Permission to assign an activity from a queue"
    name="Assign activity from queue"/>
  <typecode
    code="reporting_view"
    desc="Permission to view the Report tab, if the add-on reporting module is installed"
    name="View Report tab"/>
