    code="usercreate"
    desc="Permission to create a new user"
    name="Create users"/>
  <typecode
    code="useredit"
    desc="Permission to edit an existing user, except for roles, authority limits, or attributes"
    name="Edit users"/>
  <typecode
    code="userdelete"
    desc="Permission to delete a user (Note: if a user has had any activity it's recommended to make them non-active rather than delete)"
    name="Delete users"/>
  <typecode
    code="usereditattrs"
    desc="Permission to edit attributes for a user"
    name="Edit user attributes"/>
  <typecode
    code="usereditlang"
    desc="Permission to edit language"
    name="Edit user language"/>
  <typecode
    code="groupcreate"
    desc="Permission to create groups"
    name="Create groups"/>
  <typecode
    code="groupdelete"
    desc="Permission to delete groups"
    name="Delete groups"/>
  <typecode
    code="usergrantroles"
    desc="Permission to grant or revoke roles"
    name="Grant roles to users"/>
  <typecode
    code="rolemanage"
    desc="Permission to create, edit, or delete roles"
    name="Manage roles"/>
  <typecode
    code="zonemanage"
    desc="Permission to create, edit, or delete admin zones"
    name="Manage admin zones"/>
  <typecode
    code="zoneview"
    desc="Permission to view the list of admin zones"
    name="View admin zones"/>
  <typecode
    code="buswkmanage"
    desc="Permission to create, edit, or delete business week"
    name="Manage business week"/>
  <typecode
    code="buswkview"
    desc="Permission to view the list of business week"
    name="View business week"/>
  <typecode
    code="orgcreate"
    desc="Permission to create an organization."
    name="Create organization"/>
  <typecode
    code="orgdelete"
    desc="Permission to delete an organization."
    name="Delete organization"/>
  <typecode
    code="orgeditbasic"
    desc="Permission to edit an organization's basic info."
    name="Edit organization basic info"/>
  <typecode
    code="orgsearch"
    desc="Permission to search for organizations."
    name="Search for organization"/>
  <typecode
    code="orgviewbasic"
    desc="Permission to view an organization's basic info."
    name="View organization basic info"/>
