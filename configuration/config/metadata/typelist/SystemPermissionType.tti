  <typecode
    code="abview"
    desc="Permission to view the details of contact entries in the address book"
    name="View address book contacts"/>
  <typecode
    code="abcreate"
    desc="Permission to create a new contact in the address book"
    name="Create address book contacts"/>
  <typecode
    code="abedit"
    desc="Permission to edit an existing contact in the address book"
    name="Edit address book contacts"/>
  <typecode
    code="abviewsearch"
    desc="Permission to search contact entries in the address book"
    name="View address book contact search pages"/>
  <typecode
    code="abdelete"
    desc="Permission to delete an existing contact in the address book"
    name="Delete address book contacts"/>
  <typecode
    code="abdeletepref"
    desc="Permission to delete an existing preferred vendor address book entry"
    name="Delete address book preferred vendors"/>
  <typecode
    code="ctcview"
    desc="Permission to view and search local contact entries"
    name="View local contacts"/>
  <typecode
    code="ctccreate"
    desc="Permission to create a new local contact"
    name="Create local contacts"/>
  <typecode
    code="ctcedit"
    desc="Permission to edit an existing local contact"
    name="Edit local contacts"/>
  <!-- Address Book Tag permissions -->
  <typecode
    code="anytagcreate"
    desc="Permission to create a new contact regardless of which tag(s) it has"
    name="Create contact with any tag"/>
  <typecode
    code="anytagview"
    desc="Permission to view the details of a contact regardless of which tag(s) it has"
    name="View contact with any tag"/>
  <typecode
    code="anytagedit"
    desc="Permission to edit the details of a contact regardless of which tag(s) it has"
    name="Edit contact with any tag"/>
  <typecode
    code="anytagdelete"
    desc="Permission to delete a contact regardless of which tag(s) it has"
    name="Delete contact with any tag"/>
  <!-- Calendar permissions -->
  <typecode
    code="holidayview"
    desc="Permission to view a list of holidays or holiday details"
    name="View holidays"/>
  <typecode
    code="holidaymanage"
    desc="Permission to create, edit, and delete holidays"
    name="Manage holidays"/>
  <typecode
    code="admindatachangeview"
    desc="Permission to view the data change page."
    name="View Data Change"/>
  <typecode
    code="wsdatachangeedit"
    desc="Permission to add a data change gosu program."
    name="Add Data Change"/>
  <typecode
    code="admindatachangeexec"
    desc="Permission to execute the data change."
    name="Execute Data Change"/>
  <typecode
    code="changecontactsubtype"
    desc="Permission to change contact subtype"
    name="Change Contact Subtype"/>
  <typecode
    code="eventmessageview"
    desc="Permission to view the event messages page"
    name="View event messages"/>
