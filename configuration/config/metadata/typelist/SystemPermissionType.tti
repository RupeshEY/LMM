  <typecode
    code="usergrantauth"
    desc="Permission to grant or change an authority limit for a user"
    name="Grant authority limits"/>
  <typecode
    code="actapproveany"
    desc="Permission to approve any approval activity even if the activity is assigned to someone else; the approver is still subject to authority limit restrictions"
    name="Approve any approval activity"/>
  <typecode
    code="actview"
    desc="Permission to view activities"
    name="View activities"/>
  <!-- integration events monitor permissions -->
  <typecode
    code="integadmin"
    desc="Permission to administer integration events"
    name="Administer integration"/>
  <!-- purge permissions -->
  <typecode
    code="purge"
    desc="Permission to purge objects from the database"
    name="Purge objects"/>
  <!-- archive permissions -->
  <typecode
    code="archive"
    desc="Permission to archive objects"
    name="Archive objects"/>
  <typecode
    code="lvprint"
    desc="Permission to print listviews"
    name="Print listviews"/>
