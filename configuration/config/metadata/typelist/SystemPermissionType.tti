  <typecode
    code="ruleadmin"
    desc="Permission to run Guidewire Studio or import rules"
    name="Administer rules"/>
  <!-- Community Admin -->
  <typecode
    code="regionview"
    desc="Permission to view the list of regions and region details"
    name="View regions"/>
  <typecode
    code="roleview"
    desc="Permission to view the list of roles and role details"
    name="View roles"/>
  <typecode
    code="groupview"
    desc="Permission to view details of a group"
    name="View groups"/>
  <typecode
    code="grouptreeview"
    desc="Permission to see the user/group tree on the Administration tab"
    name="View group tree"/>
  <typecode
    code="actpatcreate"
    desc="Permission to create new activity patterns"
    name="Create activity pattern"/>
  <typecode
    code="actpatedit"
    desc="Permission to edit activity patterns"
    name="Edit activity pattern"/>
  <typecode
    code="actpatdelete"
    desc="Permission to delete activity patterns"
    name="Delete activity pattern"/>
  <typecode
    code="actpatview"
    desc="Permission to view the list of activity patterns or activity pattern details"
    name="View activity pattern"/>
  <typecode
    code="attrmanage"
    desc="Permission to create, edit, or delete user attributes"
    name="Manage attributes"/>
  <typecode
    code="attrview"
    desc="Permission to view the list of user attributes or attribute details"
    name="View attributes"/>
  <typecode
    code="alpview"
    desc="Permission to view authority limit profiles"
    name="View authority limit profiles"/>
  <typecode
    code="soapadmin"
    desc="Permission to use the SOAP APIs"
    name="SOAP administration"/>
