  <typecode
    code="notecreate"
    desc="Permission to add notes"
    name="Create notes"/>
  <typecode
    code="noteview"
    desc="Permission to view notes"
    name="View notes"/>
  <typecode
    code="noteeditbody"
    desc="Permission to edit the body of notes"
    name="Edit note body"/>
  <typecode
    code="noteedit"
    desc="Permission to edit the notes"
    name="Edit note"/>
  <typecode
    code="notedelete"
    desc="Permission to remove notes"
    name="Delete notes"/>
  <typecode
    code="actreviewassign"
    desc="Permission to review and approve manually-approved assignables"
    name="Review assignments"/>
  <typecode
    code="abcreatepref"
    desc="Permission to add a preferred vendor to the address book"
    name="Create address book preferred vendors"/>
  <typecode
    code="abeditpref"
    desc="Permission to modify an existing preferred vendor address book entry"
    name="Edit address book preferred vendors"/>
  <typecode
    code="acteditunowned"
    desc="Permission to modify (edit/skip/close) activities owned by other users"
    name="Edit unowned activities"/>
  <typecode
    code="userviewall"
    desc="Permission to see users in all visible groups"
    name="View all users"/>
