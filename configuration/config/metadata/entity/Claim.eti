    name="AgencyId"
    nullok="true"
    type="varchar">
    <columnParam
      name="size"
      value="9"/>
  </column>
  <column
    desc="True if this claim has exceeded the Reinsurance Reporting Threshold"
    name="ReinsuranceReportable"
    nullok="true"
    type="bit"/>
  <!-- New Work Comp Fields for claim loss type -->
