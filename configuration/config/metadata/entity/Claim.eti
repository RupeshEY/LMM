    name="FlaggedDate"
    nullok="true"
    type="datetime"/>
  <column
    desc="The reason this claim is flagged."
    name="FlaggedReason"
    nullok="true"
    type="mediumtext"/>
  <column
    desc="True if this is an incident report only and the claim will not be processed."
    name="IncidentReport"
    nullok="true"
    type="bit"/>
  <column
