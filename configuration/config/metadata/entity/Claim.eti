      <keyfilter
        name="Policy.PolicyType"/>
    </keyfilters>
  </typekey>
  <typekey
    desc="General cause of loss; dependent on loss type."
    name="LossCause"
    nullok="true"
    typelist="LossCause">
    <keyfilters>
      <keyfilter
        name="LossType"/>
    </keyfilters>
  </typekey>
  <typekey
    desc="High level claim type (for example, Auto or Property)."
