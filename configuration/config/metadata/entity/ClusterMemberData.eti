<?xml version="1.0"?>
<entity
  xmlns="http://guidewire.com/datamodel"
  cacheable="false"
  entity="ClusterMemberData"
  exportable="false"
  extendable="false"
  final="true"
  instrumentationtable="true"
  loadable="false"
  platform="true"
  table="clustermemberdata"
