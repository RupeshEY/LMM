<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <DetailViewPanel
    id="ClaimSnapshotExposureDV"
    mode="Content">
    <Require
      name="Claim"
      type="Claim"/>
    <Require
      name="Exposure"
      type="dynamic.Dynamic"/>
    <InputColumn>
      <InputSetRef
        def="ClaimSnapshotExposureDetailInputSet(Exposure)"
        mode="&quot;default&quot;"/>
    </InputColumn>
    <InputColumn>
      <Label
        label="DisplayKey.get(&quot;Web.ClaimSnapshotExposureDV.Default.IncidentOverview&quot;)"/>
      <TextInput
        action="ClaimSnapshotPropertyContentsIncidentPopup.push(Claim, Exposure.PropertyContentsIncident)"
        id="Contents_Incident"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotExposureDV.ContentsDamage.PropertyContentsIncident&quot;)"
        value="Exposure.PropertyContentsIncident.DisplayName"
        valueType="dynamic.Dynamic"/>
      <InputSetRef
        def="ClaimSnapshotExposureCodingInputSet(Exposure)"
        mode="&quot;default&quot;"/>
    </InputColumn>
    <InputFooterSection>
      <InputSetRef
        def="ClaimSnapshotOtherCarrierInvolvementInputSet(Exposure)"
        mode="&quot;default&quot;"/>
    </InputFooterSection>
  </DetailViewPanel>
</PCF>