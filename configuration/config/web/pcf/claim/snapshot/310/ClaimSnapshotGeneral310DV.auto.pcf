        id="MainContact"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.MainContact&quot;)"
        value="Snapshot.MainContact.DisplayName"
        valueType="dynamic.Dynamic"/>
