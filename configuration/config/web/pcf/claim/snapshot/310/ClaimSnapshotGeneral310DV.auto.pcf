        id="MainContactType"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.DriverRelation&quot;)"
        value="Snapshot.MainContactType"
        valueType="dynamic.Dynamic"/>
      <TextInput
        id="DateRptdToAgent"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.DateRptdToAgent&quot;)"
        value="util.Snapshot.renderValue(Snapshot.DateRptdToAgent)"/>
      <TextInput
        id="Fault"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.Fault&quot;)"
        value="Snapshot.Fault"
        valueType="dynamic.Dynamic"/>
    </InputColumn>
    <InputColumn>
      <Label
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.Insured&quot;)"/>
      <TextInput
        id="Vehicle"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.Vehicle&quot;)"
        value="Snapshot.Vehicle.DisplayName"
        valueType="dynamic.Dynamic"/>
      <TextInput
        id="Style"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.Style&quot;)"
        value="Snapshot.Vehicle.Style"
        valueType="dynamic.Dynamic"/>
      <TextInput
        id="Color"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.Color&quot;)"
        value="Snapshot.Vehicle.Color"
        valueType="dynamic.Dynamic"/>
      <TextInput
        id="Vin"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.Vin&quot;)"
        value="Snapshot.Vehicle.Vin"
        valueType="dynamic.Dynamic"/>
      <TextInput
        id="State"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.State&quot;)"
        value="Snapshot.Vehicle.State"
        valueType="dynamic.Dynamic"/>
      <TextInput
        id="LicensePlate"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.LicensePlate&quot;)"
        value="Snapshot.Vehicle.LicensePlate"
        valueType="dynamic.Dynamic"/>
      <InputDivider/>
      <Label
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.Driver&quot;)"/>
      <TextInput
        id="Driver"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.Driver_1&quot;)"
        value="Snapshot.Driver.DisplayName"
        valueType="dynamic.Dynamic"/>
      <TextInput
        id="VehicleUseReason"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.VehicleUseReason&quot;)"
