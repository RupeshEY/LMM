        valueType="dynamic.Dynamic"/>
      <TextInput
        id="DriverRelation2"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.DriverRelation2&quot;)"
        value="Snapshot.DriverRelation"
        valueType="dynamic.Dynamic"/>
      <InputDivider/>
      <Label
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.Claim&quot;)"/>
      <TextInput
        id="CreateTime"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.CreateTime&quot;)"
        value="util.Snapshot.renderValue(Snapshot.CreateTime)"/>
      <TextInput
        id="CloseDate"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.CloseDate&quot;)"
        value="util.Snapshot.renderValue(Snapshot.CloseDate)"/>
      <TextInput
        id="ClosedOutcome"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.ClosedOutcome&quot;)"
        value="Snapshot.ClosedOutcome"
        valueType="dynamic.Dynamic"/>
      <BooleanRadioInput
        falseLabel="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.FirstNoticeSuit&quot;)"
        id="IncidentReport"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.IncidentReport_1&quot;)"
        trueLabel="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.FirstNoticeSuit_2&quot;)"
        value="Snapshot.IncidentReport"
        valueType="dynamic.Dynamic"/>
      <BooleanRadioInput
        falseLabel="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.FirstNoticeSuit&quot;)"
        id="CoverageInQuestion"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.CoverageInQuestion_1&quot;)"
        trueLabel="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.FirstNoticeSuit_2&quot;)"
        value="Snapshot.CoverageInQuestion"
        valueType="dynamic.Dynamic"/>
      <TextInput
        id="DeductibleStatus"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.DeductibleStatus&quot;)"
        value="Snapshot.DeductibleStatus"
        valueType="dynamic.Dynamic"/>
      <TextInput
        id="LitigationStatus"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.LitigationStatus&quot;)"
        value="Snapshot.LitigationStatus"
        valueType="dynamic.Dynamic"/>
      <TextInput
        id="SubrogationStatus"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.SubrogationStatus&quot;)"
        value="Snapshot.SubrogationStatus"
        valueType="dynamic.Dynamic"/>
      <TextInput
        id="SalvageStatus"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.SalvageStatus&quot;)"
        value="Snapshot.SalvageStatus"
        valueType="dynamic.Dynamic"/>
      <TextInput
        id="OtherRecovStatus"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.OtherRecovStatus&quot;)"
        value="Snapshot.OtherRecovStatus"
        valueType="dynamic.Dynamic"/>
      <TextInput
        id="SIUStatus"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.SIUStatus&quot;)"
        value="Snapshot.SIUStatus"
        valueType="dynamic.Dynamic"/>
      <TextInput
        id="ReinsuranceStatus"
        label="DisplayKey.get(&quot;Web.ClaimSnapshotGeneral310DV.Auto.ReinsuranceStatus&quot;)"
        value="Snapshot.ReinsuranceStatus"
