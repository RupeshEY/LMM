<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="pcf.xsd">
  <LocationGroup
    canVisit="perm.User.importadmindata or perm.User.exportadmindata or perm.ScriptParameter.view"
    id="Utilities"
    title="DisplayKey.get(&quot;Web.Admin.Utilities&quot;)">
    <LocationRef
      label="DisplayKey.get(&quot;Web.Admin.ImportTab&quot;)"
      location="ImportWizard()"
      shortcut="I"/>
    <LocationRef
      label="DisplayKey.get(&quot;Web.Admin.ExportTab&quot;)"
      location="ExportData()"
      shortcut="E"/>
    <LocationRef
      location="ScriptParametersPage()"/>
    <LocationRef
      location="DataChangePage()"/>
  </LocationGroup>
</PCF>