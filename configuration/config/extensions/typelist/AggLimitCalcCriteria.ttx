  desc="AggLimitCalcCriteria defines the choices for limiting which costtypes and/or cost categories count towards aggregate limits.  Configured via aggregatelimitused-config.xml"
  name="AggLimitCalcCriteria">
  <typecode
    code="costTypesExcludingExpenses"
    desc="All incurred costs except expenses"
    name="All incurred costs except expenses"
