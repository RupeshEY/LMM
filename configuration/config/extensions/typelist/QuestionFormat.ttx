<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="How the question should be rendered in PCF"
  name="QuestionFormat">
  <!-- See also the default formats in the tl_pl_question.xml file -->
  <typecode
    code="BooleanSelect"
    desc="A boolean combo box."
    name="Boolean Select">
    <category
      code="Boolean"
      typelist="QuestionType"/>
  </typecode>
  <typecode
    code="StringTextArea"
    desc="A simple multiline text field."
    name="String Text Box">
    <category
      code="String"
      typelist="QuestionType"/>
  </typecode>
</typelistextension>