<?xml version="1.0"?>
<extension
  xmlns="http://guidewire.com/datamodel"
  entityName="Incident">
  <column
    desc="Estimated cost of the loss."
    name="LossEstimate"
    nullok="true"
    type="nonnegativecurrencyamount">
    <columnParam
      name="currencyProperty"
      value="ClaimCurrency"/>
  </column>
  <array
    arrayentity="ServiceRequest"
    desc="Service requests associated with this incident."
    exportable="false"
    name="ServiceRequests"/>
</extension>