<?xml version="1.0"?>
<plugin
  interface="IStartablePlugin"
  name="LMMConsumerService">
  <plugin-gosu
    gosuclass="gw.acc.iplm.jms.consumer.LMMConsumerService">
    <param
      name="jmsProvider"
      value="tcp://localhost:61616"/>
    <param
      name="jmsQueue"
      value="LMM_INBOUND"/>
    <param
      name="backup"
      value="Backup"/>
    <param
      name="failure"
      value="Failure"/>
  </plugin-gosu>
</plugin>
