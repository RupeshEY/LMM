<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <ToolbarButtonSet
    id="GroupDetailToolbarButtonSet">
    <Require
      name="group"
      type="Group"/>
    <EditButtons/>
    <ToolbarButton
      action="gw.api.admin.BaseAdminUtil.deleteGroup(group); AdminWelcome.go() "
      available="safeToDelete()"
      confirmMessage="DisplayKey.get(&quot;Web.Admin.GroupDetail.DeleteConfirm&quot;)"
      hideIfEditable="true"
      id="GroupDetailPage_RegionsCard_DeleteButton"
      label="DisplayKey.get(&quot;Button.Delete&quot;)"
      tooltip="safeToDelete()?&quot;&quot;:DisplayKey.get(&quot;Web.Admin.GroupDetail.DeleteTooltip&quot;)"
      visible="perm.Group.delete"/>
    <ToolbarDivider/>
    <Code><![CDATA[function safeToDelete():boolean {
  return group.SafeToDelete && group.AssignableQueues?.IsEmpty
}

]]></Code>
  </ToolbarButtonSet>
</PCF>