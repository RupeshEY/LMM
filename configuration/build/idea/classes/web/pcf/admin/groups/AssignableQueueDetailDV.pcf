<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <DetailViewPanel
    id="AssignableQueueDetailDV">
    <Require
      name="AssignableQueue"
      type="AssignableQueue"/>
    <Require
      name="Group"
      type="Group"/>
    <InputColumn>
      <Label
        label="DisplayKey.get(&quot;Web.Admin.AssignableQueueDetailDV.AssignableQueue&quot;)"/>
      <TextInput
        editable="true"
        id="Name"
        label="DisplayKey.get(&quot;Web.Admin.AssignableQueueDetailDV.Name&quot;)"
        required="true"
        value="AssignableQueue.Name"/>
      <TextInput
        editable="true"
        id="Description"
        label="DisplayKey.get(&quot;Web.Admin.AssignableQueueDetailDV.Description&quot;)"
        value="AssignableQueue.Description"/>
      <BooleanRadioInput
        editable="true"
        id="SubGroupVisible"
        label="DisplayKey.get(&quot;Web.Admin.AssignableQueueDetailDV.SubGroupVisible&quot;)"
        required="true"
        value="AssignableQueue.SubGroupVisible"/>
    </InputColumn>
  </DetailViewPanel>
</PCF>