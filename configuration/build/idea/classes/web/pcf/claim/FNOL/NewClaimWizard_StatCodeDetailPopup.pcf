<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../pcf.xsd">
  <Popup
    canEdit="true"
    id="NewClaimWizard_StatCodeDetailPopup"
    startInEditMode="!Policy.Verified"
    title="DisplayKey.get(&quot;Wizard.NewClaimWizard.NewClaimPolicyStatCode.Edit&quot;)">
    <LocationEntryPoint
      signature="NewClaimWizard_StatCodeDetailPopup(Policy : Policy, StatCode : StatCode)"/>
    <Variable
      name="Policy"
      type="Policy"/>
    <Variable
      name="StatCode"
      type="StatCode"/>
    <Screen
      id="NewClaimWizard_StatCodeDetailScreen">
      <Toolbar>
        <EditButtons
          editVisible="!Policy.Verified"/>
      </Toolbar>
      <PanelRef
        def="StatCodeDetailDV(StatCode)"/>
    </Screen>
  </Popup>
</PCF>