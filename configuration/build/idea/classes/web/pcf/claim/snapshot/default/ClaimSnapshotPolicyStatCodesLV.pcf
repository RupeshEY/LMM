<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../../../pcf.xsd">
  <ListViewPanel
    id="ClaimSnapshotPolicyStatCodesLV">
    <Require
      name="Claim"
      type="Claim"/>
    <Require
      name="SnapshotParam"
      type="dynamic.Dynamic"/>
    <RowIterator
      editable="false"
      elementName="StatCode"
      value="SnapshotParam.Policy.StatCodes"
      valueType="dynamic.Dynamic">
      <Row>
        <TextCell
          id="LineNumber"
          label="DisplayKey.get(&quot;Web.ClaimSnapshotPolicyStatCodesLV.LineNumber&quot;)"
          value="StatCode.LineNumber"
          valueType="dynamic.Dynamic"/>
        <TextCell
          id="InsuranceLine"
          label="DisplayKey.get(&quot;Web.ClaimSnapshotPolicyStatCodesLV.InsuranceLine&quot;)"
          value="StatCode.InsuranceLine"
          valueType="dynamic.Dynamic"/>
        <TextCell
          id="InsuranceSubline"
          label="DisplayKey.get(&quot;Web.ClaimSnapshotPolicyStatCodesLV.InsuranceSubline&quot;)"
          value="StatCode.InsuranceSubline"
          valueType="dynamic.Dynamic"/>
        <TextCell
          id="LocationNumber"
          label="DisplayKey.get(&quot;Web.ClaimSnapshotPolicyStatCodesLV.LocationNumber&quot;)"
          value="StatCode.LocationNumber"
          valueType="dynamic.Dynamic"/>
        <TextCell
          id="BuildingNumber"
          label="DisplayKey.get(&quot;Web.ClaimSnapshotPolicyStatCodesLV.BuildingNumber&quot;)"
          value="StatCode.BuildingNumber"
          valueType="dynamic.Dynamic"/>
        <TextCell
          id="VehicleNumber"
          label="DisplayKey.get(&quot;Web.ClaimSnapshotPolicyStatCodesLV.VehicleNumber&quot;)"
          value="StatCode.VehicleNumber"
          valueType="dynamic.Dynamic"/>
        <TextCell
          id="ClassCode"
          label="DisplayKey.get(&quot;Web.ClaimSnapshotPolicyStatCodesLV.ClassCode&quot;)"
          value="StatCode.ClassCode"
          valueType="dynamic.Dynamic"/>
        <TextCell
          id="MajorPeril"
          label="DisplayKey.get(&quot;Web.ClaimSnapshotPolicyStatCodesLV.MajorPeril&quot;)"
          value="StatCode.MajorPeril"
          valueType="dynamic.Dynamic"/>
        <TextCell
          id="State"
          label="DisplayKey.get(&quot;Web.ClaimSnapshotPolicyStatCodesLV.State&quot;)"
          value="StatCode.State"
          valueType="dynamic.Dynamic"/>
        <TextCell
          id="Notes"
          label="DisplayKey.get(&quot;Web.ClaimSnapshotPolicyStatCodesLV.Notes&quot;)"
          value="StatCode.Notes"
          valueType="dynamic.Dynamic"/>
      </Row>
    </RowIterator>
  </ListViewPanel>
</PCF>
