<?xml version="1.0"?>
<PCF
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="../../../../../pcf.xsd">
  <ExitPoint
    id="MetroURL"
    popup="true"
    url="{url}">
    <LocationEntryPoint
      signature="MetroURL(url : String)"/>
    <ExitPointParameter
      locationParam="url"
      type="String"
      urlParam="url"/>
  </ExitPoint>
</PCF>