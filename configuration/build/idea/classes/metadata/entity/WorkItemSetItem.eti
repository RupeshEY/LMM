<?xml version="1.0"?>
<delegate
  xmlns="http://guidewire.com/datamodel"
  name="WorkItemSetItem"
  platform="true">
  <implementsEntity
    name="WorkItem"/>
  <column
    desc="The work item set that owns this workitem."
    getterScriptability="doesNotExist"
    name="WorkItemSetID"
    nullok="false"
    setterScriptability="doesNotExist"
    type="softentityreference"/>
