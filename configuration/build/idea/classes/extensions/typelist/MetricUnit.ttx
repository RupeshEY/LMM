<?xml version="1.0"?>
<typelistextension
  xmlns="http://guidewire.com/typelists"
  desc="Units for claim metrics, such as days, currency, percent etc."
  name="MetricUnit">
  <typecode
    code="numeric"
    name="Numeric"
    priority="1"/>
  <typecode
    code="percent"
    name="Percent"
    priority="2"/>
  <typecode
    code="currency"
    name="Currency"
    priority="3"/>
  <typecode
    code="hours"
    name="Hours"
    priority="4"/>
  <typecode
    code="days"
    name="Days"
    priority="5"/>
</typelistextension>